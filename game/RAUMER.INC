; kopieren der Raumerdaten in den Player/Missile Bereich
; es muss mindestens einmal setraumernumber(x) aufgerufen worden sein

; (2) Startadresse player missiles
; (3) x pos
; (5) y pos
; (7) hoehe (Schatten)
; (9) raumernummer 0-21 + 4 exp. + 1 empty
; ----------------------------------------------------------------------------

@COPYRAUMER
@COPYRAUMER_IIII
      LDY #2
      LDA (@HEAP_PTR),Y ; wert holen
      TAX
      DEY
      LDA (@HEAP_PTR),Y
      TAY

; (x*256+y) ist der Wert im Heap
; Start Adresse
      STY @REG   ; Missiles Adresse
      STX @REG+1
      STY @REG+10  ; Missiles Adresse (fuer schatten)
      STX @REG+11
      INX
;      STY @REG+2 ; Player 0
;      STX @REG+3
      INX
      STY @REG+4 ; Player 1
      STX @REG+5
      INX
      STY @REG+6 ; Player 2
      STX @REG+7
      INX
      STY @REG+8 ; Player 3
      STX @REG+9

; flug-hoehe (Schattenabstand)
      LDY #7
      LDA (@HEAP_PTR),Y
      STA ?RAUMER_HEIGHT

; TODO: wird noch nicht gebraucht
; width
;      LDY #11
;      LDA (@HEAP_PTR),Y
;      STA ?RAUMER_WIDTH

; y-position
      LDY #5
      LDA (@HEAP_PTR),Y
      CLC
      ADC #32-3    ; y-position, wo player beginnt (32 - 3 leere Zeilen)
      STA ?RAUMER_YPOS

; x-position
      LDY #3
      LDA (@HEAP_PTR),Y
      CLC
      ADC #50     ; x-position, wo player beginnt (48 + 2 fuer missile)
      STA ?RAUMER_XPOS


      TAX
      TAY
      STA @RS_XPOS_PLAYER1 ; 53249   ; x-pos player 1
      DEX
      STX @RS_XPOS_PLAYER2 ; 53250   ; x-pos player 2
      DEX
      STX @RS_XPOS_MISSILE1 ; 53253   ; x-pos missile 1

;     CLC
      ADC ?RAUMER_HEIGHT
      STA @RS_XPOS_PLAYER3 ; 53251   ; x-pos player 3
      TAX
      DEX
      DEX
      STX @RS_XPOS_MISSILE3 ; 53255   ; x-pos missile 3


; addiere zum Schatten
;      CLC
      LDA @REG+8
      ADC ?RAUMER_HEIGHT
      STA @REG+8

      LDA @REG+10
      ADC ?RAUMER_HEIGHT
      STA @REG+10

; TODO: Alte Routine!
      LDX #0
      LDY ?RAUMER_YPOS
;      LDX #3

      LDX ?FIRST_EMPTY_RAUMER_LINES
      JSR ?DRAW_EMPTY_RAUMER_LINES

      LDX #0
?COPYLOOP2
      LDA (@REG),Y         ; 5 Missile
      AND @RAUMER_MASK     ; 4

?RAUMER_NUMBER_1
      ORA @RAUMER0,X       ; 5
      STA (@REG),Y         ; 6

      INX                  ; 2
?RAUMER_NUMBER_2
      LDA @RAUMER0,X       ; 5
      STA (@REG+4),Y       ; 6 player 1 (white)

      INX                  ; 2
?RAUMER_NUMBER_3
      LDA @RAUMER0,X       ; 5
      STA (@REG+6),Y       ; 6 player 2 (red)

      INX                  ; 2
      LDA (@REG+10),Y      ; 5 Missile fuer Shadow
      AND @RAUMER_MASK+3   ; 4
?RAUMER_NUMBER_4
      ORA @RAUMER0,X       ; 5
      STA (@REG+10),Y      ; 6

      INX                  ; 2
?RAUMER_NUMBER_5
      LDA @RAUMER0,X       ; 5
      STA (@REG+8),Y       ; 6 player 3 (shadow)
      INX                  ; 2
      INY                  ; 2

?RAUMER_LINES
      CPX #125             ; 2
      BCC ?COPYLOOP2       ; 3 Summe 90 Takte pro Zeile

      LDX ?SECOND_EMPTY_RAUMER_LINES
;      JSR ?DRAW_EMPTY_RAUMER_LINES
;      RTS

?DRAW_EMPTY_RAUMER_LINES
?COPYEMPTYLINES1
      LDA (@REG),Y         ; 5 Missile
      AND @RAUMER_MASK     ; 4
      STA (@REG),Y         ; 6

      LDA (@REG+10),Y      ; 5 Missile fuer Shadow
      AND @RAUMER_MASK+3   ; 4
      STA (@REG+10),Y      ; 6

      LDA #0               ; 2
      STA (@REG+4),Y       ; 6 player 1 (white)
      STA (@REG+6),Y       ; 6 player 2 (red)
      STA (@REG+8),Y       ; 6 player 3 (shadow)
      INY                  ; 2
      DEX                  ; 2
      BPL ?COPYEMPTYLINES1 ; 3 Summe  55 Takte pro Zeile
      RTS

