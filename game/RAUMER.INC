; Berechnung f�r Sterne

      LSR @REG+1
      ROR @REG
      LSR @REG+1
      ROR @REG

      TYA
      AND #~00000011
      EOR #3
      TAX
;      LDX @SHIP_HSCROL ; dieses Register nicht nutzen, flackert dann f�rchterlich

      LDA ?STAR,X
      STA MEMORY_OXYGENE_FONT+3+124*8 ; wir poken diesen Wert in den Zeichensatz hinein
      STA MEMORY_OXYGENE_FONT+4+124*8

      CLC
      LDA #<MEMORY_SHIP
      ADC @REG
      STA @REG
      LDA #>MEMORY_SHIP
      ADC @REG+1
      STA @REG+1  ; @reg sollte die obere linke ecke enthalten (vermutlich unsichtbar)

      LDX #0
?STAR_LOOP
      LDA ?STAR_POSITION,X    ; horizontale Stern Position innerhalb der Zeile
      TAY

      LDA ?STAR_POSITION+3,X
      BEQ ?NO_OLD_STAR        ; evtl. kein alter Stern zu loeschen

      STA @REG+3
      LDA ?STAR_POSITION+2,X
      STA @REG+2
      LDA #0
      STA (@REG+2),y

?NO_OLD_STAR
      CLC
      LDA @REG+1
      ADC ?STAR_POSITION+1,X  ; addiere die Vertikale Position
      STA @REG+1

      LDA (@REG),y          ; pruefen, ob es schon etwas an der Position gibt
      BEQ ?NOTHING_AT_STAR_POSITION

      LDA #0                  ; Die Position ist schon belegt, das markieren wir mit 0
      STA ?STAR_POSITION+3,X
      BEQ ?NEXT_STAR_POSITION


?NOTHING_AT_STAR_POSITION
      LDA #$7c                ; stern setzen
      STA (@REG),y

      LDA @REG                ; dessen position merken
      STA ?STAR_POSITION+2,X
      LDA @REG+1
      STA ?STAR_POSITION+3,X

?NEXT_STAR_POSITION
      INX
      INX
      INX
      INX
      CPX #44
      BNE ?STAR_LOOP

      RTS


@init_starpositions
      LDA #0
      LDX #0
?STAR_CLEAR_LOOP
      STA ?STAR_POSITION+2,X
      STA ?STAR_POSITION+3,X
      INX
      INX
      INX
      INX
      CPX #44
      BNE ?STAR_CLEAR_LOOP
      RTS

