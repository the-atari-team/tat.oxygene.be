; Hiermit legen wir fest, welcher Raumer eigentlich angezeigt wird
; um die Animation zu setzen

@SETRAUMERNUMBER
@SETRAUMERNUMBER_I
; gewuenschte Raumernummer
      LDY #1
      LDA (@HEAP_PTR),Y

; * 128 (bytes)
      ASL
      TAX

      LDA ?RAUMER_NUMBER,X    ; low
      STA @REG
      LDA ?RAUMER_NUMBER+1,X  ; high
      STA @REG+1

; Vor dem Raumer stehen 3 Bytes,
; Anzahl der ersten Leerzeilen
; Anzahl der Raumerzeilen * 5
; Anzahl der letzen Leerzeilen

      LDY #0
      LDA (@REG),Y
      STA ?FIRST_EMPTY_RAUMER_LINES
      INY
      LDA (@REG),Y
      STA ?RAUMER_LINES+1
      INY
      LDA (@REG),Y
      STA ?SECOND_EMPTY_RAUMER_LINES

      CLC
      LDA @REG
      ADC #3
      TAY
      LDA @REG+1
      ADC #0
      TAX

; Selbstmodifizierender Code, igitt!
      STY ?RAUMER_NUMBER_1+1
      STY ?RAUMER_NUMBER_2+1
      STY ?RAUMER_NUMBER_3+1
      STY ?RAUMER_NUMBER_4+1
      STY ?RAUMER_NUMBER_5+1

      STX ?RAUMER_NUMBER_1+2
      STX ?RAUMER_NUMBER_2+2
      STX ?RAUMER_NUMBER_3+2
      STX ?RAUMER_NUMBER_4+2
      STX ?RAUMER_NUMBER_5+2
      RTS

?FIRST_EMPTY_RAUMER_LINES
 .byte 0

?SECOND_EMPTY_RAUMER_LINES
 .byte 0

?RAUMER_NUMBER
 .word @RAUMER0, @RAUMER1, @RAUMER2, @RAUMER3, @RAUMER4, @RAUMER5, @RAUMER6
 .word @RAUMER7, @RAUMER8, @RAUMER9, @RAUMER10
 .word @RAUMER11, @RAUMER12, @RAUMER13, @RAUMER14, @RAUMER15, @RAUMER16, @RAUMER17, @RAUMER18
 .word @RAUMER19, @RAUMER20, @RAUMER21
 .word @EXPLOSE0, @EXPLOSE1, @EXPLOSE2, @EXPLOSE3, @EXPLOSE4
